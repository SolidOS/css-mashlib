{
  "name": "css-mashlib",
  "version": "1.0.0",
  "description": "CSS with mashlib recipe",
  "main": "index.js",
  "scripts": {
    "start": "npx community-solid-server -c ./config/config-mashlib.json -f ../css-data",
    "clean": "[ -d ../css-data ] && rm -rf ../css-data || true",
    "start:clean": "npm run clean && npm run templates && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "templates": "cp -r templates/ ./node_modules/@solid/community-server/",
    "build": "npm run templates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidos/css-mashlib.git"
  },
  "keywords": [
    "css",
    "mashlib",
    "node",
    "javascript"
  ],
  "author": "Alain Bourgeois",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solidos/css-mashlib/issues"
  },
  "homepage": "https://github.com/solidos/css-mashlib#readme",
  "dependencies": {
    "@solid/community-server": "^5.1.0",
    "mashlib": "^1.8.5"  }
}
